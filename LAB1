CREATE KEYSPACE Employee WITH REPLICATION = { 'class':'SimpleStrategy', 'replication_factor':1 };
describe keyspaces;
use Employee;
CREATE TABLE Employee_info (
Emp_Id int PRIMARY KEY.
Emp_Name text,
Date_Of_Joining timestamp,
Designation text,
SaLARY INT,
Dept_Name text
);
CREATE TABLE Employee_info ( Emp_Id int PRIMARY KEY. Emp_Name text, Date_Of_Joining timestamp, Designation text, SaLARY INT, Dept_Name text );
CREATE TABLE Employee_info ( Emp_Id int PRIMARY KEY, Emp_Name text, Date_Of_Joining timestamp, Designation text, SaLARY INT, Dept_Name text );
describe tables;
describe table Employee_info;
BEGIN BATCH
INSERT INTO employee_info (Emp_Id,Emp_Name,DateOfJoining,Designation,Salary,Dept_Name)
VALUES (1,'Sakshi','2012-03-26','HR',20000,'sales');
APPLY BATCH;
BEGIN BATCH INSERT INTO employee_info (Emp_Id,Emp_Name,DateOfJoining,Designation,Salary,Dept_Name) VALUES (1,'Sakshi','2012-03-26','HR',20000,'sales'); APPLY BATCH;
BEGIN BATCH INSERT INTO employee_info (Emp_Id,Emp_Name,Date_Of_Joining,Designation,Salary,Dept_Name) VALUES (1,'Sakshi','2012-03-26','HR',20000,'sales'); APPLY BATCH;
select * from employee_info;
exit
